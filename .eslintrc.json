{
	"env": {
		"browser": true,
		"es2021": true,
		"jquery": true,
		"node": true
	},
	"extends": ["airbnb", "plugin:prettier/recommended"],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"requireConfigFile": false,
		"sourceType": "module"
	},
	"rules": {
		"camelcase": 0,
		"import/prefer-default-export": 0,
		"no-console": 0,
		"no-plusplus": 0,
		"no-shadow": 0,
		"no-undef": 0,
		"no-underscore-dangle": 0,
		"one-var": 0,
		"prefer-const": 0,
		"prefer-template": 0,
		"quotes": 0,
		"no-lonely-if": 0
	}
}
